{
  "name": "@silent-components/youtube-api-search-typed",
  "version": "0.0.1",
  "description": "Fork of youtube-api-search-reloaded with Typescript",
  "main": "dist/bundle.cjs.js",
  "module": "dist/bundle.esm.js",
  "sideEffects": false,
  "author": "Wilfred Lopez <cubamc@gmail.com>",
  "license": "MIT",
  "keywords": [
    "youtube",
    "api",
    "search"
  ],
  "scripts": {
    "ts-build": "rimraf tsbuild && tsc --project tsconfig.json",
    "prebuild": "rimraf dist",
    "build": "npm run ts-build && rollup -c",
    "lint": "eslint '**/*.js'",
    "lint:fix": "prettier-eslint '**/*.js' --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run test",
      "npm run test:coverage",
      "git add"
    ]
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@babel/preset-env": "7.7.1",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "@types/node-fetch": "^2.5.7",
    "axios-mock-adapter": "1.17.0",
    "babel-jest": "24.9.0",
    "coveralls": "3.0.7",
    "dotenv": "^8.1.0",
    "eslint": "6.6.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "lint-staged": "9.4.3",
    "prettier-eslint": "9.0.1",
    "prettier-eslint-cli": "5.0.0",
    "rimraf": "3.0.0",
    "rollup": "1.27.2",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "^5.1.2",
    "typescript": "^3.8.3"
  }
}
